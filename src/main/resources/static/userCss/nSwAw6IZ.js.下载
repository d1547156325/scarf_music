require([],function(){var i=$();$(window).on("blur",function(){if(i){i.hide()}});$(document).on("mouseenter",".tipLayer",function(){i.show()});$(document).on("mouseenter",".songlist-list .img-wrap",function(){i=$(".btn-bg",this);i.show()}).on("mousemove",".songlist-list .img-wrap",function(){i=$(".btn-bg",this);i.show()}).on("mouseleave",".songlist-list .img-wrap",function(){i=$(".btn-bg",this);i.hide()}).on("click",".songlist-list .img-wrap .songlist-play-hook",function(){i.addClass("nohide");var s=$(this).data("listid"),n={moduleName:"songMenu"};ting.media.playSongMenu(s,n);$(this).tip().tip("tipup",{msg:"已开始播放！",iconClass:"tip-success"})}).on("click",".songlist-list .img-wrap .album-play-hook",function(){i.addClass("nohide");var s=$(this).data("albumid"),n={moduleName:"playAlbum"};ting.media.playAlbum(s,n);$(this).tip().tip("tipup",{msg:"已开始播放！",iconClass:"tip-success"})})});

