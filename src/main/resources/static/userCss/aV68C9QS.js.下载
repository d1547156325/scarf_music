$(".js-change-version").on("click",function(){if(!$.cookie("oldV")){$.cookie("oldV","true",{path:"/",expires:30})}else{$.cookie("oldV",null,{path:"/"})}var o={page:"newhome",pos:"change"};ting.logger.log("click",o);location.reload();$(this).off()});$("#fixed-panel .panel-item").on("mouseenter",function(o){$(this).find(".item-line").hide();$(this).prev().find(".item-line").hide()}).on("mouseleave",function(o){$(this).find(".item-line").show();if($(this).hasClass("idea-box")){if($(".js-go-top").is(":visible")){$(this).find(".item-line").show()}else{$(this).find(".item-line").hide()}}$(this).prev().find(".item-line").show()});var checkTopDisToShow=function(){var o=document.body.scrollTop||document.documentElement.scrollTop;if(o>760){$(".js-go-top").show();$(".js-go-top").prev().find(".item-line").show()}else{$(".js-go-top").hide();$(".js-go-top").prev().find(".item-line").hide()}};checkTopDisToShow();$(window).bind("scroll",checkTopDisToShow);$(".js-go-top").click(function(o){var e=window.opera?document.compatMode=="CSS1Compat"?$("html"):$("body"):$("html,body");e.stop().animate({scrollTop:0},200);$(this).blur();o.preventDefault();o.stopPropagation()});

